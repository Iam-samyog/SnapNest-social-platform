{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://snapnest-backend-sbse.onrender.com/api/:path*"
    },
    {
      "source": "/media/:path*",
      "destination": "https://snapnest-backend-sbse.onrender.com/media/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
